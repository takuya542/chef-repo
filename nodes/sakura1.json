{
  "run_list": [
    "mysql"
  ],
  "automatic": {
    "ipaddress": "sakura1"
  },
  "authorization": {
    "sudo": {
      "users":["onda","vagrant"],
      "groups":["onda", "vagrant"],
      "passwordless":"true",
      "sudoers_defaults": [
        "env_reset",
        "mail_badpass",
        "secure_path=\"/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin\""
      ]
    }
  },
  "nginx":{
    "version": "1.4.1",
    "install_method": "source",
    "source": {
      "version": "1.4.1"
    },
    "worker_processes": 2
  }
}
